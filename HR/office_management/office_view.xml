		            <field name="type">form</field>
		            <field name="arch" type="xml">
	           		     <form string="Conference" version="7.0">
		                	<sheet>
				                <h1>
			                        <label string="Office Equipments"/>
			                    	<field name="loc_id" class="oe_inline" readonly="1" string="ID -"/>
			                    </h1>
			                    <group>
									<group col="2">	                	
					                	<field name="name" />
