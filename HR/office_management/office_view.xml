					                	<field name="name" invisible="1"/>
					                	<field name="location_id" on_change="onchange_location(location_id)" />
					                </group>
					                <newline/>
					                <group col="4">
					                <field name="task_lines" nolabel="1">
					                	<tree editable="bottom">
