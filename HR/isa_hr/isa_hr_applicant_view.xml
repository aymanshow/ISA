				<tree editable="bottom" >
					<field name="seq_num"/>
					<field name="name" on_change="onchange_name(name)"/>
					<field name="email_id"/>
					<field name="joing_attachment"/>
				 </tree>
			   </field>
		  </group>
		</page>	
  </notebook>
			        <separator string="Application Summary" attrs="{'invisible':[('state','!=','draft')]}"/>
                <field name="description" placeholder="Feedback of interviews..." attrs="{'invisible':[('state','!=','draft')]}"/>
			   
			   </sheet>
            <div class="oe_chatter">
                <field name="message_follower_ids" widget="mail_followers"/>
                <field name="message_ids" widget="mail_thread"/>
            </div>
			    
		</form>
		</field>
    </record>
    
    
    <record id="interview_manager_tree" model="ir.ui.view">
			<field name="name">Hr Applicant</field>
			<field name="model">hr.applicant</field>
			<field name="type">tree</field>
			<field name="arch" type="xml">
				<tree string="Interview Information Scheduled">
					<field name='name'/>
						<field name='employee_id'/>
						<field name='department_id'/>
						<field name='email_from'/>
				</tree>
			</field>
		</record>
    
    <record id="interview_manager_form" model="ir.ui.view">
			<field name="name">hr applicant</field>
			<field name="model">hr.applicant</field>
			<field name="type">form</field>
			<field name="arch" type="xml">
				<form string="Interview Information Scheduled" version="7.0">
			         <sheet>
					<group colspan="4" col="4">
					     <field name='name'/>
						<field name='employee_id' on_change="onchange_employee_id(employee_id)"/>
						<field name='department_id'/>
						<field name='email_from'/>
					</group>
			         </sheet>
					</form>
			</field>
