		<record model="ir.ui.view" id="view_hr_evaluation_interview_tree1123">
            <field name="name">survey.test.line.tree</field>
            <field name="model">survey.test.line</field>
            <field name="arch" type="xml">
                <tree string="Technical Test">
                    <field name="date_deadline" string="Deadline Date" invisible="1"/>
                    <field name="survey_id" string="Exam Name"/>
                    <field name="total_marks1"/>
                    <field name="min_score1"/>
                     <field name='marks'/>
