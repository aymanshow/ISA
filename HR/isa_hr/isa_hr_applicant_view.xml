<?xml version ='1.0'?>
<openerp>
    <data>
        <!-- Form view of hr.job with unique id-->
	<record model="ir.ui.view" id="recruitment_interview_calendar">
        <field name="name">Recruitment Interview</field>
        <field name="model">hr.applicant</field>
        
        <field name="priority" eval="1"/>
        <field name="arch" type="xml">
            <calendar string="Interviews Calendar" date_start="date" date_stop="date" color="user_id" mode="month">
                         <field name='name'/>
						<field name='employee_id'/>
						<field name='department_id'/>
						<field name='email_from'/>
            </calendar>
        </field>
    </record>
    
    
<record id="action_interview_calender_view" model="ir.actions.act_window">
			<field name="name">Interview Schedule</field>
			<field name="res_model">hr.applicant</field>
			<field name="view_type">form</field>
			<field name="auto_refresh">10</field>
			<field name="context" >{'default_is_interview_scheduled':True}</field>
			<field name="domain">[('is_interview_scheduled','=',True)]</field>
			<field name="view_mode">calendar,form,tree</field>
			<field name="view_id" ref="recruitment_interview_calendar"/>
		</record>
        <record id="drishti_hr_job_tree" model="ir.ui.view">
            <field name="name">ISA Hr Applicant Tree View</field>
            <field name="model">hr.applicant</field>
            <field name="type">tree</field>
            <field name="inherit_id" ref='hr_recruitment.crm_case_tree_view_job'/>
            <field eval="1" name="priority"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='partner_name']" position="after">
                  <field name='candidate_id'/>
                  <field name='state'/>
                      <field name='requestion_id'/>
                </xpath>
            </field>
        </record>
        <record model="ir.ui.view" id="hr_recruitment.crm_case_form_view_job">
        <field name="name">Jobs - Application Form</field>
        <field name="model">hr.applicant</field>
        <field name="arch" type="xml">
          <form string="Jobs - Recruitment Form" version="7.0" >
            <header>
             <button name="action_send_joining_process_email" type="object"
                        string="Email" attrs="{'invisible':[('state','!=','joining_process')]}" help="For Joining Process mail is seding to the applicant and employee"/>             
                <button name="case_close_with_emp" string="Hire &amp; Create Employees" type="object"
                        class="oe_highlight"
                        attrs="{'invisible':[('state','!=','joining_process')]}"/>
            <button name="action_create_unique_id" string="ShortList This Candidate" attrs="{'invisible':[('state','!=','draft')]}" type="object" class="oe_highlight"/>
                
                <button name="action_done" type="object" string="Done" help="Approve this  applicant for the next stage" attrs="{'invisible':[('state','!=','reference_check')]}"/>
                <button name="action_submit" type="object" string="Submit" help="Submit the document for  this  applicant for the next stage" attrs="{'invisible':[('state','!=','document_submission')]}"/>
                
                <button name="action_approve" type="object" string="Approve" help="Approve this  applicant for the next stage" attrs="{'invisible':[('state','in',['interview','director_approved','document_submission','reference_check','offer_acceptance','medical_test','joining_process','cancel','draft','done'])]}"/>
                <button name="action_accepted" type="object" string="Accepted" attrs="{'invisible':[('state','!=','offer_acceptance')]}"/>
                <button name="action_cleared" type="object" string="Pass" attrs="{'invisible':[('state','!=','medical_test')]}"/>
            <button name="action_pass" type="object" string="Pass" attrs="{'invisible':[('state','in',['test','document_submission','reference_check','offer_acceptance','medical_test','joining_process','cancel','draft','done'])]}"/>
           
           <button name="%(action_drishti_hr_rej_state_id1)d" string="Reject" type="action" help="Rejected Candidate" attrs="{'invisible':[('state','in',['done','cancel','draft'])]}"/>
             <field name="stage_id" widget="statusbar" attrs="{'readonly':[('state','in',['test','interview','reference_check','document_submission','offer_acceptance','medical_test','joining_process','done'])]}"/>
                <field name="emp_id" invisible="1"/>
            </header>
            <sheet>
                <div class="oe_right oe_button_box">`
                
                    <button name="action_makeMeeting" type="object"
                        string="Schedule Meeting" help="Schedule interview with this applicant" attrs="{'invisible':[('state','!=','draft')]}"/>
              <button name="%(action_interview_calender_view)d" type="action" 
                        string="Interview Schedule" attrs="{'invisible':[('state','!=','interview')]}" help="Person whose taking the interview"/>
               <button name="action_interview_send" type="object"
                        string="Email" attrs="{'invisible':[('state','!=','interview')]}" help="For Interview scheduled for the candidate"/>
               <button name="action_calender" type="object"
                        string="Appointment Schedule" attrs="{'invisible':[('state','!=','medical_test')]}" help="For Medical Test Appointment scheduled for the applicant"/>
                <button name="action_medical_inform_send" type="object"
                        string="Email" attrs="{'invisible':[('state','!=','medical_test')]}" help="For Medical Test mail is seding to the applicant and doctor"/>
                <button name="action_quotation_send3" type="object"
                        string="Email" attrs="{'invisible':[('state','!=','done')]}"/>
                 <button name="action_send_test_email" type="object"
                        string="Email" attrs="{'invisible':[('state','!=','test')]}"/>
             <button name="action_offer_acceptance_mail" type="object"
                        string="Email" attrs="{'invisible':[('state','!=','offer_acceptance')]}"/>
              </div>
                 <div class="oe_title"> 
                 <group colspan="4" attrs="{'invisible':[('state','!=','document_submission')]}">
                 
                <field name="photographs" widget="image" img_height="100" img_width="100"/> 
                </group>
               
                    <label for="requestion_id" class="oe_inline"/>  
                    <h2><field name="requestion_id" required="True" on_change="onchange_requestion_id(requestion_id)" attrs="{'readonly':[('state','in',['test','interview','reference_check','document_submission','director_approved','offer_acceptance','medical_test','joining_process','done'])]}"/></h2>
                    <label for="candidate_id" class="oe_inline"/>  
                    <h2>
                  <field name="candidate_id" attrs="{'readonly':[('state','in',['draft','test','interview','reference_check','document_submission','director_approved','offer_acceptance','medical_test','joining_process','done'])]}" />
                    </h2>
                     <label for="partner_name" class="oe_inline" /> 
                    <h2>
                        <field name="partner_name" required="True" class="oe_inline" attrs="{'readonly':[('state','in',['test','interview','reference_check','document_submission','director_approved','offer_acceptance','medical_test','joining_process','done'])]}"/>
                    </h2>
                    <label for="name" class="oe_inline"/> 
                    <h2><field name="name" attrs="{'readonly':[('state','in',['test','interview','reference_check','document_submission','director_approved','offer_acceptance','medical_test','joining_process','done'])]}"/></h2>
                      <label for="identification" class="oe_inline" attrs="{'invisible':[('state','!=','done')]}"/>  
                    <h2><field name="identification" attrs="{'invisible':[('state','!=','done')]}"/></h2>
			
				
				
            <group colspan="2" col="4" attrs="{'invisible':[('state','!=','document_submission')]}">
			<field name="no_children"/>
     		<field name="bank_account_no"/>		
			<field name="social_security_no"/>
			<field name="declarations"/>
			</group>
                  </div> 
			<group colspan="4" cols="4" attrs="{'invisible':[('state','!=','medical_test')]}">
                      <field name="doctor_name" domain="[('title', '=' ,'Doctor')]"/>
                  </group>
                 <group colspan="4" cols="4" attrs="{'invisible':[('state','not in',['director_approved','offer_acceptance','medical_test','joining_process','done'])]}">
                <label for="over_all_comment" class="oe_inline" attrs="{'readonly':[('state','!=','director_approved')]}"/>
               	<h3>
               	<field name="over_all_comment" attrs="{'readonly':[('state','!=','director_approved')]}" string="Director Approved"/>
                </h3>
                </group>
                <group attrs="{'invisible':[('state','!=','draft')]}">
                    <group >
                        <field name="partner_id"
                                on_change="onchange_partner_id(partner_id)"/>
                        <field name="email_from" widget="email" required="True"/>
                        <field name="partner_phone"/>
                        <field name="partner_mobile"/>
                        <field name="type_id" placeholder="Degree"/>
                    </group>
                    <group >
                        <field name="user_id" invisible="1"/>
                        <field name="password" invisible="1"/>
                        <label for="title_action"/>
                        <div>
                            <field name="date_action"/>
                            <field name="title_action" class="oe_inline" placeholder="e.g. Call for interview"/>
                        </div>
                        <field name="priority"/>
                        <field name="state" invisible="1" />
                        <field name="source_id"/>
                        <field name="reference"/>
                    </group>
                    <group string="Job"  attrs="{'invisible':[('state','!=','draft')]}">
                        <field name="survey" invisible="1"/>
                        <field name="response" invisible="1"/>
                        <field name="job_id"/>
                        <field name="department_id" on_change="onchange_department_id(department_id)" required="True"/>
                        <label for="availability"/>
                        <div>
                            <field name="availability" class="oe_inline"/> <label string="Day(s)" class="oe_inline"/>
                        </div>
                    </group>
                    <group string="Contract" attrs="{'invisible':[('state','!=','draft')]}">
                        <label for="salary_expected"/>
                        <div>
                            <field name="salary_expected" class="oe_inline"/>
                            <span class="oe_inline" attrs="{'invisible':[('salary_expected_extra','=',False)]}"> + </span>
                            <field name="salary_expected_extra" class="oe_inline" placeholder="Extra advantages..."/>
                        </div>
                        <label for="salary_proposed"/>
                        <div>
                            <field name="salary_proposed" class="oe_inline"/>
                            <span class="oe_inline" attrs="{'invisible':[('salary_proposed_extra','=',False)]}"> + </span>
                            <field name="salary_proposed_extra" class="oe_inline" placeholder="Extra advantages..."/>
                        </div>
                    </group>
                </group>
                <group attrs="{'invisible':[('state','!=','draft')]}">
                    <field name="categ_ids" widget="many2many_tags"/>
                </group>
                
 <notebook>
 	
<page string="Working Experience">
            <group colspan="4" cols="4">
                <field name="work_experiane_line" colspan="4" nolabel="1" attrs="{'readonly':[('state','!=','draft')]}">
				<tree editable="bottom" >
					<field name="name"/>
					<field name="cargo_exercised"/>
					<field name="responsibility"/>
					<field name="start_date"/>
					<field name="finish_date"/>
			   </tree>
			   </field>
		  </group>
		</page>
<page string="Education">
            <group colspan="4" cols="4">
                <field name="education_line" colspan="4" nolabel="1" attrs="{'readonly':[('state','!=','draft')]}">
				<tree editable="bottom" >
					<field name="name"/>
					<field name="qualification_obtain"/>
					<field name="start_date"/>
					<field name="finish_date"/>
			   </tree>
			   </field>
		  </group>
		</page>
<page string="Professional Qualification">
            <group colspan="4" cols="4">
                <field name="professional_line" colspan="4" nolabel="1" attrs="{'readonly':[('state','!=','draft')]}">
				<tree editable="bottom" >
					<field name="name"/>
					<field name="institute"/>
					<field name="start_date"/>
					<field name="finish_date"/>
			   </tree>
			   </field>
		  </group>
		</page>  
<page string="Honors /Award" >
            <group colspan="4" cols="4">
                <field name="honor_awards_line" colspan="4" nolabel="1" attrs="{'readonly':[('state','!=','draft')]}">
				<tree editable="bottom" >
					<field name="name"/>
					<field name="local"/>
					<field name="date"/>
			   </tree>
			   </field>
		  </group>
		</page>
<page string="Language Spoken" >
            <group colspan="4" cols="4">
                <field name="language_spoken_line" colspan="4" nolabel="1" attrs="{'readonly':[('state','!=','draft')]}">
				<tree editable="bottom" >
					<field name="name"/>
					<field name="basic"/>
					<field name="intermediate"/>
					<field name="advance"/>
			   </tree>
			   </field>
		  </group>
		</page>

<page string="Interview" attrs="{'invisible':[('state','not in',['interview','reference_check','document_submission','director_approved','offer_acceptance','medical_test','joining_process','done'])]}">
            <group colspan="4" cols="4">
                <field name="inter_line" colspan="4" nolabel="1" attrs="{'readonly':[('state','!=','interview')]}">
				<tree editable="bottom" >
					<field name="seq_num"/>
					<field name="quest"/>
					<field name="rating"/>
					<field name="comment"/>
			   </tree>
			   </field>
			   </group>
			   </page>
<page string="Test Details" attrs="{'invisible':[('state','not in',['test','interview','reference_check','document_submission','director_approved','offer_acceptance','medical_test','joining_process','done'])]}">
					
					<field nolabel="1" name="survey_ids">
					<tree editable="bottom">
					<field name="survey_id" string="Exam Name" readonly="1"/>
					<field name="total_marks1" readonly="1"/>
					<field name="min_score1" readonly="1"/>
					<field name="response" readonly="1"/>
					<field name="marks" readonly="1"/>
					 <field name="comment"/>
                     <button name="%(survey.action_view_survey_question_message)d" string="Answer Survey" states="waiting_answer" type="action" icon="gtk-execute" context="{'survey_id': survey_id, 'response_id': [response], 'response_no':0, 'active' : response,'request' : True, 'object' : 'survey.test.line', 'cur_id' : active_id}" attrs="{'readonly':[('survey_id','=',False)]}"/>
                     <button name="%(survey.survey_browse_response)d" string="Print Interview" states="done" type="action"  icon="gtk-print" context="{'survey_id': survey_id, 'response_id' : [response], 'response_no':0,}" attrs="{'readonly':[('response','=',False)]}" />
                    <field name="state"/>
                    <button string="Done" name="survey_req_done" type="object" states="waiting_answer" icon="gtk-jump-to"/>
                     <button string="Send Request" name="survey_req_waiting_answer" states="draft" type="object" icon="gtk-yes" />
					<field name="attach"/>
					<field name="user_id" string="Interviewer" invisible="1"/>
                     </tree>
                      </field>
		</page>
<page string="Reference Check Form" attrs="{'invisible':[('state','not in',['reference_check','document_submission','director_approved','offer_acceptance','medical_test','joining_process','done'])]}">
            <group colspan="4" cols="4">
                <field name="reference_check_line" colspan="4" nolabel="1" attrs="{'readonly':[('state','!=','reference_check')]}">
				<tree editable="bottom" >
					<field name="serial_number"/>
					<field name="type"/>
					<field name="name"/>
					<field name="phone_num"/>
					<field name="email" widget="email"/>
					<field name="company"/>
					<field name="check_mode"/>
					<field name="feedback"/>
			   </tree>
			   </field>
		  </group>
		</page>
		<page string="Documents" attrs="{'invisible':[('state','not in',['document_submission','director_approved','offer_acceptance','medical_test','joining_process','done'])]}">
        <notebook>	    
<page string="Personal" attrs="{'invisible':[('state','not in',['document_submission','director_approved','offer_acceptance','medical_test','joining_process','done'])]}">
            <group colspan="2" cols="4">
					<field name="medical_certificate" attrs="{'readonly':[('state','!=','document_submission')]}"/>
					<field name="residence_prove" attrs="{'readonly':[('state','!=','document_submission')]}"/>
					<field name="id_copy" attrs="{'readonly':[('state','!=','document_submission')]}"/>
					<field name="passport_copy" attrs="{'readonly':[('state','!=','document_submission')]}"/>
					<field name="drives_licenses" attrs="{'readonly':[('state','!=','document_submission')]}"/>
					<field name="working_visa" attrs="{'readonly':[('state','!=','document_submission')]}"/>
					<field name="id_gaurdian" attrs="{'readonly':[('state','!=','document_submission')]}"/>
					<field name="health_insurance_card" attrs="{'readonly':[('state','!=','document_submission')]}"/>
		  </group>
	 </page>
		

<page string="Criminal" attrs="{'invisible':[('state','not in',['document_submission','director_approved','offer_acceptance','medical_test','joining_process','done'])]}">
            <group colspan="4" cols="4">
                <field name="criminal_record_line" colspan="4" nolabel="1" attrs="{'readonly':[('state','!=','document_submission')]}">
				<tree editable="bottom" >
					<field name="description"/>
					<field name="crime_attachment"/>
			   </tree>
			   </field>
		  </group>
		</page>
<page string="Family" attrs="{'invisible':[('state','not in',['document_submission','director_approved','offer_acceptance','medical_test','joining_process','done'])]}">
            <group colspan="4" cols="4">
                <field name="family_information_line" colspan="4" nolabel="1" attrs="{'readonly':[('state','!=','document_submission')]}">
				<tree editable="bottom" >
					<field name="child_name"/>
					<field name="child_birth_cert"/>
				 </tree>
			   </field>
		  </group>
		</page>
<page string="Others" attrs="{'invisible':[('state','not in',['document_submission','director_approved','offer_acceptance','medical_test','joining_process','done'])]}">
            <group colspan="4" cols="4">
                <field name="other_certificate_line" colspan="4" nolabel="1" attrs="{'readonly':[('state','!=','document_submission')]}">
				<tree editable="bottom" >
					<field name="desc"/>
					<field name="name"/>
			   </tree>
			   </field>
		  </group>
		</page>
		</notebook>
		</page>
		

<page string="Offer Acceptance" attrs="{'invisible':[('state','not in',['offer_acceptance','medical_test','joining_process','done'])]}">
            <group colspan="4" cols="4">
                <field name="offer_acceptance_line" colspan="4" nolabel="1" attrs="{'readonly':[('state','!=','offer_acceptance')]}">
				<tree editable="bottom" >
				       <field name="seq_num"/>
                      <field name="prob_joing_date"/>
                       <field name="offer_latter_acceptance_date"/>
                      <field name="joining_status"/>
                      <field name="attach_offer_latter"/>
				 </tree>
			   </field>
		  </group>
		</page>
		
<page string="Medical Test" attrs="{'invisible':[('state','not in',['medical_test','joining_process','done'])]}">
            <group colspan="4" cols="4">
                <field name="medical_test_line" colspan="4" nolabel="1" attrs="{'readonly':[('state','!=','medical_test')]}">
				<tree editable="bottom" >
					<field name="seq_num"/>
					<field name="type_of_test"/>
					<field name="result"/>
				 </tree>
			   </field>
		  </group>
		</page>

<page string="Joining Process" attrs="{'invisible':[('state','not in',['joining_process','done'])]}">
            <group colspan="4" cols="4">
