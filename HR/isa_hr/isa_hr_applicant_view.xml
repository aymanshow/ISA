    <record id="action_interview_form" model="ir.actions.act_window.view">
    <field eval="2" name="sequence"/>
			<field name="view_mode">form</field>
			<field name="view_id" ref="interview_manager_form"/>
			<field name="act_window_id" ref="action_interview_calender_view"/>
		</record>
		
		<record id="action_interview_manager_id" model="ir.actions.act_window">
			<field name="name">Hr Applicant Interviewer Information</field>
