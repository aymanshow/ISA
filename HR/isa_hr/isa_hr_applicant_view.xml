			<field name="view_type">form</field>
			<field name="view_mode">tree</field>
			<field name="view_id" ref="interview_manager_tree"/>
		</record>
	
                  
 
  		<record id="drishti_hr_state_test_tree1" model="ir.ui.view">
			<field name="name">test.stage.tree</field>
			<field name="model">test.stage.form</field>
			<field name="type">tree</field>
